<body class="" style="height: 100%;">
<div class="row" style="height: 20%">
    <h2 style="background: #4a148c;color: white">President Race</h2>
</div>
<div class="row" style="height: 60%;margin:0 auto;padding: 20px">
    <div class="card col-lg-{{lg}} candidate">
        <div class="card-header card-header-primary">
            <h4 class="card-title">{{candidate_name}}</h4>
            <p class="card-category"></p>
        </div>
        <div class="card-body">
            <img class="candidate_pic" src="/assets/img/ur_01.jpg" alt="">
            <hr>
            <div style="width: 30%;height: 30%;color: #9c27b0;" class="bar"></div>
        </div>
    </div>
    <div style="height: 100px;vertical-align: middle" class="col-lg-3">
        <div class="row">
            <div class="card col">
                <div class="card-header card-header-primary">
                    <h4 class="card-title">Total Votes</h4>
                    <p class="card-category"></p>
                </div>
                <div class="">


                    <h3 style="margin: 0 auto;font-size: 60px">45,000</h3>
                </div>
            </div>

        </div>
        <div class="row">
            <div class="countdown">

            </div>
        </div>

    </div>
    <div class="card col-lg-{{lg}} candidate">
        <div class="card-header card-header-primary">
            <h4 class="card-title">{{candidate_name}}</h4>
            <p class="card-category"></p>
        </div>
        <div class="card-body">
            <img class="candidate_pic" src="/assets/img/ud_01.jpg" alt="">
            <hr>
            <div style="width: 30%;height: 30%;color: #9c27b0;" class="bar"></div>
        </div>
    </div>
</div>


